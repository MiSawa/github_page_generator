        // '\\Z': '{\\mathbb{Z}}',
        '\\Q': '{\\mathbb{Q}}',
        // '\\R': '{\\mathbb{R}}',
        '\\C': '{\\mathbb{C}}',
        '\\divides': '{\\mid}',
        '\\abs': '{\\left| #1 \\right|}',
        '\\relmiddle': '\\mathrel{}\\middle#1\\mathrel{}',
        '\\setmid': '\\relmiddle|',
        '\\set': '\\left\\{\\,#1\\,\\right\\}',
      },
    });
